{
  "name": "safe-log-reader",
  "description": "Safe Log Reader",
  "version": "0.9.5",
  "private": false,
  "keywords": [
    "log",
    "reader",
    "tail",
    "watch"
  ],
  "homepage": "https://github.com/DoubleCheck/safe-log-reader",
  "bugs": {
    "url": "https://github.com/DoubleCheck/safe-log-reader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DoubleCheck/safe-log-reader.git"
  },
  "dependencies": {},
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-eslint": "^17.0.0",
    "grunt-mocha-test": "^0.12.7",
    "grunt-version-check": "^0.2.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "travis-cov": "^0.2.5"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 88
    }
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "coverage": "NODE_ENV=test COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter travis-cov",
    "coveralls": "COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "optionalDependencies": {}
}
